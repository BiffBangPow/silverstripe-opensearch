  "homepage": "https://github.com/BiffBangPow/silverstripe-opensearch",
  "keywords": [
    "silverstripe",
    "search"
  ],
  "authors": [
    {
      "name": "Tim Burt",
      "email": "tim@biffbangpow.com"
    },
